{  "manifest_version": 2,  "name": "crx test hehe",  "description": "This extension shows a Google Image search result for the current page",  "version": "1.0",  "background": {     "scripts": ["background.js"]  },  "browser_action": {    "default_icon": "icon.png",    "default_popup": "popup.html",    "default_title": "fucking, hehe!"  },  "content_scripts": [    {      "matches": ["http://miku.us/"],      "css": ["mystyles.css"],      "js": ["contentscript.js"]    },    {      "matches": ["*://*.youku.com/*"],      "js": ["capture.js"]    }  ],  "options_page": "options.html",  "permissions": [    "tabs"  ]}